{
  "name": "@m31271n/black-box",
  "version": "0.6.6",
  "description": "Toolbox for Black Engine.",
  "license": "MIT",
  "repository": "m31271n/black-box",
  "author": {
    "name": "m31271n",
    "email": "m31271n@pm.me",
    "url": "stack.m31271n.com"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
    "doc": "npm run update-index && esdoc",
    "test": "ava",
    "code-quality": "npm run lint && npm run test",
    "cleanup": "rm -rf dist",
    "update-index": "./scripts/update-index.js",
    "copy-files": "mkdir -p dist && cp -r src/* package.json README.md LICENSE dist/",
    "build": "npm run cleanup && npm run update-index && npm run copy-files",
    "release": "npm run code-quality && npm run build && cd dist/ && npm publish"
  },
  "files": [
    "**/*"
  ],
  "main": "index.js",
  "keywords": [
    ""
  ],
  "dependencies": {
    "black-engine": "https://github.com/MassiveHeights/Black.git#v0.6.x-dev",
    "qs": "^6.5.2",
    "vconsole": "^3.2.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^10.0.1",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-ava": "^5.0.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.11.1"
  },
  "lint-staged": {
    "linters": {
      "**/*.{json,md}": [
        "prettier --write",
        "git add"
      ],
      "**/*.js": [
        "prettier --write",
        "eslint",
        "git add"
      ]
    }
  }
}
