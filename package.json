{
  "name": "@m31271n/black-box",
  "version": "0.0.5",
  "description": "Toolbox for Black Engine.",
  "license": "MIT",
  "repository": "m31271n/black-box",
  "author": {
    "name": "m31271n",
    "email": "m31271n@pm.me",
    "url": "stack.m31271n.com"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
    "test": "ava",
    "code-quality": "npm run lint && npm run test",
    "cleanup": "rm -rf dist",
    "transpile": "babel src -d dist/",
    "copy-files": "cp package.json README.md LICENSE dist/",
    "build": "npm run cleanup && npm run transpile && npm run copy-files",
    "release": "npm run code-quality && npm run build && cd dist/ && npm publish"
  },
  "files": [
    "**/*"
  ],
  "main": "index.js",
  "keywords": [
    ""
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "ava": "^0.25.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-ava": "^5.0.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.11.1"
  },
  "lint-staged": {
    "linters": {
      "**/*.{json,md}": [
        "prettier --write",
        "git add"
      ],
      "**/*.js": [
        "prettier --write",
        "eslint",
        "git add"
      ]
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
